	<ul class="thumbnails">
	  <li class="span12">
	    <div class="thumbnail">
	    	<div class="row-fluid">
	    		<div class="span10">
	    			<h2><%= link_to_unless_current post.title, post %></h2>
	    		</div>
	    		<div class="span2">

	    			<p> <%= time_ago_in_words(post.created_at) %> ago</p>
	    		</div>
	    	</div>
	    	<div class="row-fluid">
	    		<div class="span12">
	    			<p><%= raw post.body %></p>
	    		</div>
	    	</div>
	    </div>
	  </li>
	</ul>
